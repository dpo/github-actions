    "eslint": "^7.17.0",
    "eslint-plugin-jest": "^24.1.10",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "2.2.1",
    "ts-jest": "^26.5.3",
    "typescript": "^4.2.3"
